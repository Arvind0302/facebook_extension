{
    "manifest_version": 3,
    "name": "Facebook_Friends_Data",
    "version": "1.0",
    "description": "This is an extension to fetch Facebook friends data.",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
      ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }